{
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES2019",
      "outDir": "dist",
      "pretty": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "declaration": true,
      "declarationMap": true,
      "incremental": true,
      "sourceMap": true
    },
    "exclude": [
      "node_modules",
    ],
    "include": [
      "src"
    ]
  }